        <mxCell id="R9wKPlAbwoIMhmEVUMdv-36" value="&lt;div&gt;&lt;span&gt;Параметр &quot;&lt;/span&gt;&lt;b&gt;Цель&lt;/b&gt;&lt;span&gt;&quot;:&lt;/span&gt;&lt;/div&gt;&lt;p style=&quot;margin: 0in 0in 0.0001pt&quot;&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: &amp;#34;helvetica&amp;#34; , sans-serif ; color: black&quot;&gt;ипотека (-2%)&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;color: black ; font-family: &amp;#34;helvetica&amp;#34; , sans-serif&quot;&gt;развития бизнеса (-0.5%)&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;color: black ; font-family: &amp;#34;helvetica&amp;#34; , sans-serif&quot;&gt;потребительский кредит&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;color: black ; font-family: &amp;#34;helvetica&amp;#34; , sans-serif&quot;&gt;(+1.5%)&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;div&gt;&lt;font color=&quot;#000000&quot; face=&quot;helvetica, sans-serif&quot;&gt;Параметр &lt;b&gt;&quot;Кредитный рейтинг&quot;:&lt;/b&gt;&lt;/font&gt;&lt;/div&gt;&lt;p&gt;&lt;/p&gt;&lt;p&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: &amp;#34;helvetica&amp;#34; , sans-serif ; color: black&quot;&gt;кредитный рейтинг = ( -1)&amp;nbsp; &amp;nbsp;+1.5%&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: &amp;#34;helvetica&amp;#34; , sans-serif ; color: black&quot;&gt;кредитный рейтинг = 0&amp;nbsp; &amp;nbsp; 0%&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family: &amp;#34;helvetica&amp;#34; , sans-serif ; color: black&quot;&gt;кредитный рейтинг = 1&amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/span&gt;-0.25%&lt;/li&gt;&lt;/ul&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;color: rgb(0 , 0 , 0) ; font-family: &amp;#34;helvetica&amp;#34; , sans-serif&quot;&gt;кредитный рейтинг = 2&amp;nbsp; &amp;nbsp; -0.75%&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;p&gt;&lt;/p&gt;&lt;/div&gt;&lt;p&